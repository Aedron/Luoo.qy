{
  "name": "luoo.qy",
  "version": "0.0.3",
  "description": "luoo.qy",
  "main": "main.js",
  "dependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.2.10",
    "electron": "^1.6.1",
    "nedb": "^1.8.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "reactcss": "^1.1.1",
    "request-promise": "^4.1.1",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "electron-devtools-installer": "^2.1.0",
    "electron-rebuild": "^1.5.7",
    "electron-winstaller": "^2.5.2",
    "react-addons-pure-render-mixin": "^15.4.2"
  },
  "scripts": {
    "start": "electron-rebuild && electron main.js",
    "build-all": "electron-packager . Luoo.qy --all --ignore=node_modules/electron-* --overwrite --rebuild",
    "build-mac": "electron-packager . Luoo.qy --platform=darwin --arch=x64 --icon=static/pic/macOS.icns --ignore=node_modules/electron-* --overwrite --rebuild",
    "build-win": "electron-packager . Luoo.qy --platform=win32 --arch=x64 --icon=static/pic/win.ico --ignore=node_modules/electron-* --overwrite --rebuild",
    "build-linux": "electron-packager . Luoo.qy --platform=linux --arch=x64 --icon=static/pic/win.ico --ignore=node_modules/electron-* --overwrite --rebuild"
  },
  "keywords": [
    "luoo.qy"
  ],
  "author": "https://github.com/HuQingyang",
  "license": "ISC",
  "ecmaFeatures": {
    "modules": true
  }
}
