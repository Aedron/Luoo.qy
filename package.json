{
  "name": "luoo.qy",
  "productName": "luoo.qy",
  "version": "2.0.0",
  "description": "",
  "main": "./dist",
  "scripts": {
    "start:view": "cd ./view; NODE_ENV=development npm start",
    "pack": "NODE_ENV=production node ./build",
    "build": "npm run build:main; npm run build:view",
    "build:main": "rm -rf ./dist/*; ./node_modules/.bin/parcel build ./app/index.ts --target electron --bundle-node-modules --no-source-maps --no-cache",
    "build:view": "rm -rf ./dist/view; cd ./view; NODE_ENV=production npm run build; cp -r ./build ../dist/view",
    "lint": "eslint app --color --fix",
    "lint:view": "eslint view --color --fix",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "keywords": [],
  "author": "qingyang",
  "license": "MIT"
}
